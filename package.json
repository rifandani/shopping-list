{
  "name": "shop-list",
  "version": "1.0.0",
  "description": "Simple Full-Stack Shopping List berbasis MERN Stack dengan Redux",
  "main": "server.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "Heroku",
    "Express",
    "MongoDB",
    "React",
    "Node",
    "Redux"
  ],
  "author": "Tri Rizeki Rifandani",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.2.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.12",
    "mongoose-timestamp": "^0.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  }
}
